{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "yt-dlp-x",
  "version": "2025.11.5",
  "identifier": "com.apple.yt-dlp-x",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "yt-dlp-x",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/logo.png"
    ]
  }
}
